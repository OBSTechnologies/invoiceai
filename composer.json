{
    "name": "obstechnologies/invoiceai",
    "description": "AI-powered invoice parsing and management for Laravel applications",
    "keywords": ["laravel", "invoice", "ai", "ocr", "claude", "anthropic", "parsing"],
    "license": "MIT",
    "authors": [
        {
            "name": "OBS Technologies",
            "email": "support@obstechnologies.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "anthropic-ai/sdk": "^0.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "OBSTechnologies\\InvoiceAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OBSTechnologies\\InvoiceAI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OBSTechnologies\\InvoiceAI\\InvoiceAIServiceProvider"
            ],
            "aliases": {
                "InvoiceAI": "OBSTechnologies\\InvoiceAI\\Facades\\InvoiceAI"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
